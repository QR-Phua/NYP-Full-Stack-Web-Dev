	{{!-- QR JS files --}}
	<script src="https://js.stripe.com/v3/"></script>
	<script src="/js/validation.js"></script>

	<script src="/js/cart.js"></script>
	<script src="/js/add_to_cart.js"></script>
	<script src="/js/custorder_All.js"></script>
	<script src="/js/custorder_Info.js"></script>


	<title>Chun Mee Lee's Rattan Furniture</title>

</head>

<body>
	{{>_navbar}}
	<div class="container" id="template_Container">
		{{>_msg}} {{!-- views/partials/_msg.handlebars --}}
		{{>_errors}} {{!-- views/partials/_errors.handlebars --}}
		{{> _flashmsg}} {{!-- views/partials/_flashmsg.handlebars --}}

		{{{body}}}
		{{>_footer}}
	</div>
	<button onclick="topFunction()" id="myBtn"><i class="fas fa-arrow-circle-up"></i></button>

	<script>
		//Get the button
		var mybutton = document.getElementById("myBtn");

		// When the user scrolls down 20px from the top of the document, show the button
		window.onscroll = function () { scrollFunction() };

		function scrollFunction() {
			if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
				mybutton.style.display = "block";
			} else {
				mybutton.style.display = "none";
			}
		}

		// When the user clicks on the button, scroll to the top of the document
		function topFunction() {
			document.body.scrollTop = 0;
			document.documentElement.scrollTop = 0;
		}
	</script>
	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
	<script src="/js/main.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
		integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
		crossorigin="anonymous"></script>
	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
		integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
		crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap-confirmation2/dist/bootstrap-confirmation.min.js"></script>
	<script>
		$('[data-toggle=confirmation]').confirmation({
			rootSelector: '[data-toggle=confirmation]',
		});
	</script>

	<!-- Google Map API Library -->
	<script
		src="https://maps.googleapis.com/maps/api/js?key={{googleMapsApiKey}}&libraries=places&callback=activatePlacesSearch"></script>

	<!-- Google Pay API -->
	<script src="http://pay.google.com/gp/p/js/pay.js" onload="onGooglePayLoaded()"></script>
	<style>
		#template_Container {
			padding-top: 70px;
		}

		#myBtn {
			display: none;
			position: fixed;
			bottom: 20px;
			right: 30px;
			z-index: 99;
			font-size: 18px;
			border: none;
			outline: none;
			cursor: pointer;
			padding: 15px;
			border-radius: 4px;
		}

		#myBtn:hover {
			color: white;
			background-color: #555;
		}
	</style>
	<footer></footer>
</body>

</html>